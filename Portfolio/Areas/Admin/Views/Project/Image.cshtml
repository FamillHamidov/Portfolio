@model EntityLayer.Dto.ProjectDto
@{
    ViewData["Title"] = "Image";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<form asp-action="Image" method="post" class="form-group" enctype="multipart/form-data">
	<input type="hidden" asp-for="Id" />
	<input type="hidden" asp-for="ProjectName" />
	<input type="hidden" asp-for="FileName" />
	<input type="hidden" asp-for="Description" />
	<input type="hidden" asp-for="ProjectUrl" />
	<div class="form-group">
		<label asp-for="Picture" class="col-md-2 control-label"></label>
		<div class="col-md-10">
			<input asp-for="Picture" type="file" class="form-control" />
		</div>
	</div>
	<div class="form-group">
		<button class="btn btn-primary">Update</button>
	</div>
</form>


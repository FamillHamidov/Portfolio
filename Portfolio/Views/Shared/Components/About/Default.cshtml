@using EntityLayer.Entities
@model List<About>
<div class="container py-5">
    <div class="row">
        @foreach (var item in Model)
        {
             <div class="col-lg-4 py-3">
            <div class="img-place wow fadeInUp">
                <img src="@item.PictureUrl" alt="">
            </div>
        </div>
        <div class="col-lg-6 offset-lg-1 wow fadeInRight">
            <h1 class="fw-light">@item.Name</h1>
            <h5 class="fg-theme mb-3">@item.Title</h5>
            <p class="text-muted">@item.Summary</p>
            <ul class="theme-list">
                <li><b>From:</b> @item.From</li>
                <li><b>Lives In:</b> @item.LivesIn</li>
                <li><b>Age:</b> @item.Age</li>
                <li><b>Gender:</b> @item.Gender</li>
            </ul>
            <button class="btn btn-theme-outline">Download CV</button>
        </div>
        }
       
    </div>
</div>
